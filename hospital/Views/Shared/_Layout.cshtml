                        <a href="@Url.Action("Index", "Home")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">主畫面</span></a>
                    </li>
                    <li data-username="dashboard Default Ecommerce CRM Analytics Crypto Project" class="nav-item @(ViewContext.RouteData.Values["Controller"].ToString()== "Home" & ViewContext.RouteData.Values["Action"].ToString() == "Files_Download" ? "active" : "")">
                        <a href="@Url.Action("Files_Download", "Home")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-download"></i></span><span class="pcoded-mtext">說明文件下載</span></a>
                    </li>
                    <li class="nav-item pcoded-menu-caption">
                        <label>Conversion</label>
