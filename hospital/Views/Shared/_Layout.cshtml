                    </li>
                    <li data-username="dashboard Default Ecommerce CRM Analytics Crypto Project" class="nav-item @(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "IPD" ? "active" : "")">
                        <a href="@Url.Action("IPD", "Conv")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-file-text"></i></span><span class="pcoded-mtext">住院</span></a>
                    </li>
                    <li data-username="dashboard Default Ecommerce CRM Analytics Crypto Project" class="nav-item @(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "LAB" ? "active" : "")">
                        <a href="@Url.Action("LAB", "Conv")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-file-text"></i></span><span class="pcoded-mtext">檢驗檢查</span></a>
                    </li>
                    <li data-username="dashboard Default Ecommerce CRM Analytics Crypto Project" class="nav-item @(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "CRLF" ? "active" : "")">
                        <a href="@Url.Action("CRLF", "Conv")" class="nav-link "><span class="pcoded-micon"><i class="feather icon-file-text"></i></span><span class="pcoded-mtext">癌登表</span></a>
                    </li>
                    @*<li data-username="basic components Button Alert Badges breadcrumb Paggination progress Tooltip popovers Carousel Cards Collapse Tabs pills Modal Grid System Typography Extra Shadows Embeds" class="nav-item pcoded-hasmenu">
                            <a href="javascript:" class="nav-link "><span class="pcoded-micon"><i class="feather icon-file-text"></i></span><span class="pcoded-mtext">資料轉換</span></a>
                            <ul class="pcoded-submenu">
                                <li class="@(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "OPD" ? "active" : "")"><a href="@Url.Action("OPD", "Conv")" class="">門診</a></li>
                                <li class="@(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "IPD" ? "active" : "")"><a href="@Url.Action("IPD", "Conv")" class="">住院</a></li>
                                <li class="@(ViewContext.RouteData.Values["Controller"].ToString()== "Conv" & ViewContext.RouteData.Values["Action"].ToString() == "LAB" ? "active" : "")"><a href="@Url.Action("LAB", "Conv")" class="">檢驗檢查</a></li>
                            </ul>
                        </li>*@
                </ul>
            </div>
        </div>
    </nav>
    <!-- [ navigation menu ] end -->
    <!-- [ Header ] start -->
    <header class="navbar pcoded-header navbar-expand-lg navbar-light">
        <div class="m-header">
            <a class="mobile-menu" id="mobile-collapse1" href="javascript:"><span></span></a>
            <a href="@Url.Action("Index", "Home")" class="b-brand">
                <div class="b-bg">
                    <i class="feather icon-trending-up"></i>
                </div>
                <span class="b-title">篩選系統</span>
            </a>
        </div>
        <a class="mobile-menu" id="mobile-header" href="javascript:">
            <i class="feather icon-more-horizontal"></i>
        </a>
        <div class="collapse navbar-collapse">
            <ul class="navbar-nav ml-auto"></ul>
        </div>
    </header>
    <!-- [ Header ] end -->
