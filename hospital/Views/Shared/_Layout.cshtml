<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 北慈撈選系統</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <!-- fontawesome icon -->
    <link rel="stylesheet" href="@Url.Content("~/Content/assets/fonts/fontawesome/css/fontawesome-all.min.css")">
    <!-- animation css -->
    <link rel="stylesheet" href="@Url.Content("~/Content/assets/plugins/animation/css/animate.min.css")">
    <!-- vendor css -->
    <link rel="stylesheet" href="@Url.Content("~/Content/assets/css/style.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap-tagsinput/bootstrap-tagsinput.css")">
    @RenderSection("Styles", required: false)
