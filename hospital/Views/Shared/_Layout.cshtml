        <hr />
        <footer>
            <p>
                &copy; @DateTime.Now.Year &nbsp;國家級人體生物資料庫整合平台團隊製作
            </p>
        </footer>
    </div>

    <!-- Required Js -->
    @Scripts.Render("~/bundles/Required")
    @Scripts.Render("~/bundles/pcoded")
    @Scripts.Render("~/Content/bootstrap-tagsinput")
    @*@Scripts.Render("~/bundles/jquery")*@
    @RenderSection("Scripts", required: false)
</body>
</html>
